alias sharpersist {
    # Construct the command to execute Rubeus
    local('$command');
    $command = "C:\\Tools\\SharPersist\\SharPersist\\bin\\Release\\SharPersist.exe";

    # Execute the command in the beacon
    btask($1, "Executing Sarpersist assembly.");
    beacon_execute_assembly($1, $exe_path, "");
}

# Register a custom command
beacon_command_register("sharpersist", "Execute SharPersist", "Runs SharPersist.exe from the specified path.");
