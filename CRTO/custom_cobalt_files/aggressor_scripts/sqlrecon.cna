alias sqlrecon {
    # Construct the command to execute Rubeus
    local('$command');
    $command = "C:\\Tools\\SQLRecon\\SQLRecon\\bin\\Release\\SQLRecon.exe";

    # Execute the command in the beacon
    btask($1, "Executing SQLRecon assembly.");
    beacon_execute_assembly($1, $exe_path, "");
}

# Register a custom command
beacon_command_register("sqlrecon", "Execute SQLRecon", "Runs SQLRecon.exe from the specified path.");
