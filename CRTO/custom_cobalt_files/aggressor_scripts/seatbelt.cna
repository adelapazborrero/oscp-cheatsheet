alias seatbelt {
    # Construct the command to execute Rubeus
    local('$command');
    $command = "C:\\Tools\\Seatbelt\\SeatBelt\\bin\\Release\\SeatBelt.exe";

    # Execute the command in the beacon
    beacon_input($1, $command);
}

# Register a custom command
beacon_command_register("seatbelt", "Execute Seatbelt", "Runs Seatbelt.exe from the specified path.");
