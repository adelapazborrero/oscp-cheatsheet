alias certify {
    # Construct the command to execute Rubeus
    local('$command');
    $command = "C:\\Tools\\Certify\\Certify\\bin\\Release\\Certify.exe";

    # Execute the command in the beacon
    beacon_input($1, $command);
}

# Register a custom command
beacon_command_register("certify", "Execute Certify", "Runs Certify.exe from the specified path.");
